{
    "name": "widevine",
    "version": "1.0.4",
    "description": "NodeJS Widevine Client",
    "keywords": [
        "widevine"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/Frooastside/node-widevine.git"
    },
    "author": "Frooastside <frooastside@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Frooastside/node-widevine/issues"
    },
    "homepage": "https://github.com/Frooastside/node-widevine/",
    "scripts": {
        "proto:compile": "npx buf generate proto",
        "lint": "eslint --ext ts ./src --quiet",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/esm/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@bufbuild/buf": "^1.66.0",
        "@bufbuild/protoc-gen-es": "^2.11.0",
        "@eslint/js": "^10.0.1",
        "@trivago/prettier-plugin-sort-imports": "^6.0.2",
        "@types/node": "^25.3.3",
        "dotenv": "^17.3.1",
        "eslint": "^10.0.2",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^17.3.0",
        "jiti": "^2.6.1",
        "prettier": "^3.8.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.1"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.11.0"
    }
}
